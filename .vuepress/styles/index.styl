@import url('https://fonts.googleapis.com/css?family=Gugi')

/* jay jin*/

h1 {
  font-family: "Gugi", cursive;
  font-weight: 500;
  border-bottom: 17px solid #ffd816;
  border-bottom: 17px solid lightblue;
  word-break: keep-all;
  font-size: 5rem;
}

@media (max-width: 550px) {
  h1 {
    font-size: 4.4rem;
  }
}

@media (max-width: 430px) {
  h1 {
    font-size: 3.4rem;
  }
}